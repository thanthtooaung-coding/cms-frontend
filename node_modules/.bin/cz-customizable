#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/alvin/GitHub/cms-frontend/node_modules/.pnpm/cz-customizable@7.4.0/node_modules/cz-customizable/node_modules:/home/alvin/GitHub/cms-frontend/node_modules/.pnpm/cz-customizable@7.4.0/node_modules:/home/alvin/GitHub/cms-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/alvin/GitHub/cms-frontend/node_modules/.pnpm/cz-customizable@7.4.0/node_modules/cz-customizable/node_modules:/home/alvin/GitHub/cms-frontend/node_modules/.pnpm/cz-customizable@7.4.0/node_modules:/home/alvin/GitHub/cms-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cz-customizable/standalone.js" "$@"
else
  exec node  "$basedir/../cz-customizable/standalone.js" "$@"
fi
